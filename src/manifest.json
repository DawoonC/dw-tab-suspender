{
  "name": "DW Tab Suspender",
  "description": "Simple Google Chrome tab suspender",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "alarms"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/sleep_16.png",
      "32": "/images/sleep_32.png",
      "48": "/images/sleep_48.png",
      "128": "/images/sleep_128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "/images/sleep_16.png",
    "32": "/images/sleep_32.png",
    "48": "/images/sleep_48.png",
    "128": "/images/sleep_128.png"
  }
}
